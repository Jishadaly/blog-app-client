{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["client/**/*"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "client/dist/$1"
      }
    ]
  }
  